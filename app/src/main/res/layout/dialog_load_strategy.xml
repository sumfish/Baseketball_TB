<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:gravity="center">
    <ImageView
        android:layout_width="240dp"
        android:layout_height="60dp"
        android:background="@drawable/banner_load_tactic"
        />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_marginTop="10dp">

        <TextView
            android:id="@+id/testTextView_size"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            />

        <Spinner
            android:id="@+id/category_spinner_in_load"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:gravity="center"
            android:entries="@array/strategy_category">


        </Spinner>

        <Button
            android:id="@+id/save_strategy_button3"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/btn_edit"
            android:layout_marginLeft="20dp"/>


    </LinearLayout>
    <RelativeLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="horizontal"
        android:layout_marginTop="30dp">
        <!--
        <ListView
            android:id="@+id/listview_id_in_load"
            android:layout_width="70dp"
            android:background="#303841"
            android:layout_height="wrap_content"
            />
            -->
        <ListView
            android:id="@+id/listView_in_load"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center">

            </ListView>


    </RelativeLayout>
</LinearLayout>
