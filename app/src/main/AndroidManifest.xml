<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mislab.tacticboard"
    android:versionCode="1"
    android:versionName="1.0"
    android:largeHeap="true" android:installLocation="auto">
	<uses-permission
     android:name="android.permission.READ_EXTERNAL_STORAGE" />
	<uses-permission 
     android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.INTERNET"/>
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-sdk
        android:minSdkVersion="19"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.mislab.tacticboard.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <!--<activity android:name=".MultiActivity"></activity>
        <activity android:name=".ThreeDMainActivity"></activity>-->
    </application>

</manifest>
